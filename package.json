{
  "name": "css-quickdraw-redux",
  "version": "1.0.0",
  "description": "CSS Selector Multiplayer Puzzle",
  "main": "webpack.config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "coffeelint": "find . ! -path \"./node_modules/*\" -path \"*.coffee\" | xargs ./node_modules/coffeelint/bin/coffeelint",
    "dev": "NODE_ENV=dev npm start",
    "dev-debug": "NODE_ENV=dev NODE_PATH=./app coffee --nodejs --inspect index.coffee 2>>cssqderr.log",
    "dev-test": "NODE_ENV=test npm start",
    "dev-test-debug": "NODE_ENV=test NODE_PATH=./app coffee --nodejs --debug-brk index.coffee 2>>cssqderr.log",
    "prepush": "npm run coffeelint && npm test",
    "prod": "NODE_ENV=prod npm start",
    "start": "NODE_PATH=./app coffee index.coffee",
    "test": "npm run test-backend && npm run test-frontend",
    "test-backend": "NODE_ENV=test NODE_PATH=./ mocha --compilers coffee:coffee-script/register --require ./test/chai.coffee --recursive --reporter spec",
    "test-frontend": "node ./node_modules/karma/bin/karma start ./karma.conf.coffee --single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DaQuirm/css-quickdraw-redux.git"
  },
  "author": "",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/DaQuirm/css-quickdraw-redux/issues"
  },
  "homepage": "https://github.com/DaQuirm/css-quickdraw-redux",
  "engines": {
    "node": "6.9.1"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "chai": "^3.5.0",
    "co-mocha": "^1.1.3",
    "coffee-lint-loader": "DaQuirm/coffeelint-loader",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.11.1",
    "coffeelint": "^1.16.0",
    "css-loader": "^0.25.0",
    "electron-prebuilt": ">=0.35.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "husky": "^0.11.9",
    "karma": "^1.3.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-electron-launcher": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^1.8.0",
    "lolex": "^1.5.2",
    "mocha": "^3.1.2",
    "node-libs-browser": "^1.0.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "stylint": "^1.5.7",
    "stylint-loader": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "webpack": "^1.13.3",
    "webpack-notifier": "^1.4.1"
  },
  "dependencies": {
    "bower": "^1.8.2",
    "bunyan": "^1.8.1",
    "co": "^4.6.0",
    "cson": "^3.0.1",
    "cssqd-shared": "file:./shared",
    "electron": "^1.4.6",
    "koa": "^1.1.0",
    "koa-bodyparser": "^2.0.1",
    "koa-ejs": "^3.0.0",
    "koa-passport": "^1.2.0",
    "koa-router": "^5.2.3",
    "koa-session": "^3.3.1",
    "koa-static": "^1.5.1",
    "mongoose": "^4.6.8",
    "nconf": "^0.7.1",
    "nexus-node": "DaQuirm/nexus",
    "nexus-warp": "DaQuirm/nexus-warp#378dd82e",
    "passport": "^0.3.0",
    "passport-facebook": "^2.0.0",
    "passport-github": "^1.0.0",
    "passport-google-auth": "^1.0.0",
    "passport-twitter": "^1.0.3"
  }
}
