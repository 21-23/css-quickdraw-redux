    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^1.6.0",
    "husky": "^0.8.1",
    "karma": "^0.12.37",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
