{
  "name": "css-quickdraw-redux",
  "version": "1.0.0",
  "description": "CSS Selector Multiplayer Puzzle",
  "main": "webpack.config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepush": "npm run coffeelint && npm test",
    "test-backend": "NODE_ENV=test NODE_PATH=./ mocha --compilers coffee:coffee-script/register --require ./test/chai.coffee --recursive --reporter spec",
    "test-frontend": "node ./node_modules/karma/bin/karma start ./karma.conf.coffee --single-run",
    "test": "npm run test-backend && npm run test-frontend",
    "start": "NODE_PATH=./app coffee index.coffee",
    "dev": "NODE_ENV=dev npm start",
    "dev-test": "NODE_ENV=test npm start",
    "debug": "node-inspector 2>/dev/null && NODE_PATH=./app NODE_ENV=dev coffee --nodejs --debug-brk index.coffee",
    "coffeelint": "find . ! -path \"./node_modules/*\" -path \"*.coffee\" | xargs ./node_modules/coffeelint/bin/coffeelint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DaQuirm/css-quickdraw-redux.git"
  },
  "author": "",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/DaQuirm/css-quickdraw-redux/issues"
  },
  "homepage": "https://github.com/DaQuirm/css-quickdraw-redux",
  "devDependencies": {
    "autoprefixer-loader": "^2.0.0",
    "chai": "^3.4.0",
    "co-mocha": "^1.1.2",
    "coffee-lint-loader": "0.0.1",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.9.3",
    "coffeelint": "^1.13.0",
    "css-loader": "^0.15.1",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^1.6.0",
    "husky": "^0.8.1",
    "karma": "^0.12.37",
    "karma-chai-sinon": "^0.1.5",
    "karma-electron-launcher": "git+https://github.com/lele85/karma-electron-launcher.git",
    "karma-mocha": "^0.2.0",
    "karma-webpack": "^1.5.1",
    "lolex": "^1.3.2",
    "mocha": "^2.3.3",
    "node-libs-browser": "^0.5.2",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.12.3",
    "stylus-loader": "^1.2.1",
    "webpack": "^1.9.11",
    "webpack-notifier": "^1.2.1"
  },
  "dependencies": {
    "co": "^4.6.0",
    "cson": "^3.0.1",
    "cssqd-shared": "file:./shared",
    "koa": "^1.1.0",
    "koa-bodyparser": "^2.0.1",
    "koa-passport": "^1.2.0",
    "koa-router": "^5.2.3",
    "koa-session": "^3.3.1",
    "koa-static": "^1.5.1",
    "mongoose": "^4.2.7",
    "nconf": "^0.7.1",
    "nexus-node": "git+https://github.com/DaQuirm/nexus.git",
    "nexus-warp": "git+https://github.com/DaQuirm/nexus-warp.git",
    "nightmare": "^2.0.7",
    "passport": "^0.3.0",
    "passport-facebook": "^2.0.0",
    "passport-github": "^1.0.0",
    "passport-google-auth": "^1.0.0",
    "passport-twitter": "^1.0.3",
    "vo": "^1.0.3"
  }
}
