{
  "name": "css-quickdraw-redux",
  "version": "1.0.0",
  "description": "CSS Selector Multiplayer Puzzle",
  "main": "webpack.config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepush": "npm run coffeelint && npm test",
    "test": "node ./node_modules/karma/bin/karma start ./karma.conf.coffee --single-run",
    "start": "NODE_PATH=./app coffee service.coffee",
    "dev": "NODE_ENV=dev npm start & webpack -w",
    "debug": "node-inspector 2>/dev/null && NODE_PATH=./app NODE_ENV=dev coffee --nodejs --debug-brk service.coffee",
    "coffeelint": "find . ! -path \"./node_modules/*\" -path \"*.coffee\" | xargs ./node_modules/coffeelint/bin/coffeelint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DaQuirm/css-quickdraw-redux.git"
  },
